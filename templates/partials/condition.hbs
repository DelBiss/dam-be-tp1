<div >
        <div>
            <div>
                <div class="d-inline fw-bold">Température</div>
                <div class="d-inline">{{#if temp_C}}{{temp_C}}{{else}}{{tempC}}{{/if}}℃</div>
            </div>
            <div>
                <div class="d-inline">Ressentie</div>
                <div class="d-inline">{{FeelsLikeC}}℃</div>
            </div>
        </div>
        <div>
            <div class="d-inline fw-bold">Conditions</div>
            <div class="d-inline">{{weatherDesc.[0].value}}</div>
        </div>
        <div>
            <div class="d-inline fw-bold">Vents</div>
            <div class="d-inline">{{windspeedKmph}} Km/h</div>
            <div  class="d-inline" data-degree="{{winddirDegree}}">
                {{winddir16Point}}
            </div>
        </div>
        <div>
            <div class="d-inline fw-bold">UV</div>
            <div class="d-inline">{{uvIndex}}</div>
        </div>
        <div>
            <div class="d-inline fw-bold">Humidité</div>
            <div class="d-inline">{{humidity}}%</div>
        </div>
        <div>
            <div class="d-inline fw-bold">Précipitations</div>
            <div class="d-inline">{{precipMM}} mm</div>
        </div>
    </div>